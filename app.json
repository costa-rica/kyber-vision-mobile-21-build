{
	"expo": {
		"name": "kyber-vision-mobile-21",
		"slug": "kyber-vision-mobile-21",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./src/assets/expo-assets/icon.png",
		"scheme": "kybervisionmobile20ts",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./src/assets/expo-assets/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			},
			"edgeToEdgeEnabled": true
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./src/assets/expo-assets/favicon.png"
		},
		"plugins": [
			"expo-router",
			[
				"expo-splash-screen",
				{
					"image": "./src/assets/expo-assets/splash-icon.png",
					"imageWidth": 200,
					"resizeMode": "contain",
					"backgroundColor": "#ffffff"
				}
			]
		],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"router": {},
			"eas": {
				"projectId": "fcebacc8-78f7-43b3-ac47-5c2c2633da01"
			}
		},
		"owner": "costa-rica",
		"runtimeVersion": {
			"policy": "appVersion"
		},
		"updates": {
			"url": "https://u.expo.dev/fcebacc8-78f7-43b3-ac47-5c2c2633da01"
		}
	}
}
