{
	"expo": {
		"name": "kyber-vision-mobile-21",
		"slug": "kyber-vision-mobile-21-build",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./src/assets/icon.png",
		"scheme": "kybervisionmobile21build",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"ios": {
			"icon": "./src/assets/icon.png",
			"supportsTablet": true,
			"bundleIdentifier": "com.costarica.kybervisionmobile21build",
			"infoPlist": {
				"ITSAppUsesNonExemptEncryption": false
			}
		},
		"android": {
			"icon": "./src/assets/icon.png",
			"adaptiveIcon": {
				"foregroundImage": "./src/assets/icon.png",
				"backgroundColor": "#ffffff"
			},
			"edgeToEdgeEnabled": true,
			"package": "com.costarica.kybervisionmobile21build"
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./src/assets/expo-assets/favicon.png"
		},
		"plugins": [
			"expo-router",
			[
				"expo-splash-screen",
				{
					"image": "./src/assets/expo-assets/splash-icon.png",
					"imageWidth": 200,
					"resizeMode": "contain",
					"backgroundColor": "#ffffff"
				}
			]
		],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"router": {},
			"eas": {
				"projectId": "d9e867ce-4255-4722-87d4-2ba2c5779f90"
			}
		},
		"owner": "costa-rica",
		"runtimeVersion": {
			"policy": "appVersion"
		},
		"updates": {
			"url": "https://u.expo.dev/d9e867ce-4255-4722-87d4-2ba2c5779f90"
		}
	}
}
